<div class="fixed-width-lg flexbox-container" id="search">
  <div class="form">
    Buscar
    <br>
    <br>
    <form action="#">
      <div>
        <label for="keywords">Palabras Clave: </label>
        <input type="text" name="keywords"  class="float-r">
      </div>
      <br>
      <div>
        <label for="state">State: </label>
        <select name="state" id="state" class="float-r">
          <option value="true">True</option>
          <option value="false">False</option>
          <option value="proccess">Proccess</option>
        </select>
      </div>
      <br>
      <div>
        <label for="region">Region: </label>
        <select name="region" id="region" class="float-r">
          <option value="Santiago">Santiago</option>
          <option value="Temuco">Temuco</option>
        </select>
      </div>
      <br>
      <div>
        <label for="category">Categoria: </label>
        <select name="category" id="category" class="float-r">
          <option value="sport">Deporte</option>
          <option value="electronics">Tecnologia</option>
        </select>
      </div>
      <br>
      <div>
        <input type="submit" name="Submit"  value="Buscar">
      </div>
    </form>
  </div>
  <div class="form">
    <h2> Objetos </h2>
    <div class="flexbox-item objects border">
      <% if (!objectsList.length) { %>
        <p>No results</p>
      <% } else { %>
        <table>
          <thead>
            <tr>
              <th>Nombre</th>
              <th>Categoria</th>
              <th>Due√±o</th>
              <th>Estado</th>
              <th>Ver</th>
            </tr>
          </thead>
          <tbody>
            <% objectsList.forEach((object) => { %>
              <tr>
                <td><%= object.name %></td>
                <td><%= object.category.name %></td>
                <td><%= object.user.username %></td>
                <td><%= object.state %></td>
                <td><a href="<%= view(object) %>">Ver</a></td>
              </tr>
            <% }) %>
          </tbody>
        </table>
      <% } %>
    </div>
  </div>
</div>