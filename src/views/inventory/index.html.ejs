<div class="grid-container">
  <div class="inventory-title"><span>Mi inventario</span></div>
<% objectsList.forEach((object) => { %>
  <a class="grid-item" href="<%= objectView(object) %>">
  <% if (object.photos.length > 0) { %>
    <img src="https://xchangestorage.s3.us-east-2.amazonaws.com/<%=object.photos[0].fileName%>">
  <% } else { %>
    <img src="https://xchangestorage.s3.us-east-2.amazonaws.com/no_disponible.jpg">
  <% }%>
    <div class="object-info">
      <div class="object-name"> <%= object.name %> </div>
      <div class="object-description max-lines"> <%= object.description %> </div>
      <div class="object-category"> <span><span class="text-style">Categor√≠a:</span> <%= object.category.name %></span></div>
      <div class="object-views"><span><span class="text-style">Vistas:</span> <%= object.views %></span> </div>
      <div class="object-state"><span><span class="text-style">Estado:</span> <%= object.state ? 'Disponible' : 'No Disponible' %></span></div>
    </div>
  </a>
<% }) %>
<% if (objectsList.length < 9) { %>
  <% for (var i = 0; i < 9 - objectsList.length; i++) { %>
    <div class="grid-item"></div>
  <% } %>
<% } %>
</div>