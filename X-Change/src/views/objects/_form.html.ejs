<div>
  <% if (locals.errors) { %>
    <div class="errors">
      You need to fix the following errors:
      <ul>
        <% errors.forEach(error => { %>
          <li><%= error.message %></li>
        <% }); %>
      </ul>
    </div>
  <% } %>
 <form action="<%= submitObjectPath %>" method="post">
   <% if (!object.isNewRecord) { %>
     <input type="hidden" name="_method" value="patch" />
   <% } %>
   <div class="field">
     <label for="userId">userId</label>
     <input type="text" name="userId" value="<%= object.userId %>"  />
   </div>
   <div class="field">
     <label for="categoryId">categoryId</label>
     <input type="text" name="categoryId" value="<%= object.categoryId %>"  />
   </div>
   <div class="field">
     <label for="name">Name</label>
     <input type="text" name="name" value="<%= object.name %>"  />
   </div>
   <div class="field">
     <label for="state">state</label>
     <input type="text" name="state" value="<%= object.state %>"  />
   </div>
   <div class="field">
     <label for="description">description</label>
     <input type="text" name="description" value="<%= object.description %>"  />
   </div>
   <div class="field">
     <% if (object.isNewRecord) { %>
       <input type="submit" name="create" value="Create"  />
     <% } else {%>
       <input type="submit" name="update" value="Update"  />
     <% } %>
   </div>
 </form>
</div>
